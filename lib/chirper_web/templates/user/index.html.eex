<h4>#USERS</h4>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col">Profile</th>
    </tr>
  </thead>
  <tbody>
<%= for user <- @users do %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.username %></td>
      <td><%= link "Profile", to: Routes.user_path(@conn, :show, user) %></td>
    </tr>
<% end %>
  </tbody>
</table>
